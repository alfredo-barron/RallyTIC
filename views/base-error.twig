<!doctype html>
<html lang="en" class="error-page">
<head>
 <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{{ title }}</title>
  <link rel="shortcut icon" href="{{ asset('RallyTIC.png') }}" type="image/x-icon">
  <!-- PACE js -->
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <link rel="stylesheet" href="{{ asset('pace-theme-minimal.css') }}">
  <script src="{{ asset('pace.min.js') }}" data-pace-options='{ "ajax": false, "restartOnRequestAfter": false }'></script>
  <!-- CSS -->
  <link rel="stylesheet" href="{{ asset('materialize.css') }}">
  {% block css %}{% endblock %}
  <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>
<body class="grey lighten-4">
  <header>
    {% include "navbar.twig" %}
  </header>
  <main>
    <div class="container">
      <div class="row">{% block content %}{% endblock %}</div>
    </div>
  </main>
  {% block parallax %}{% endblock %}
  <footer class="page-footer light-blue darken-3">
    <div class="container">
      <div class="row">
        <div class="col l12 s12">
          <h5 class="white-text">Rally TIC</h5>
          <p class="grey-text text-lighten-4">Programación Móvil & Programación Web Avanzada <br> Verano 2015 </p>
        </div>
      </div>
    </div>
    <div class="footer-copyright">
      <div class="container">
      © {{ now|date('Y') }} Rally TIC
      </div>
    </div>
  </footer>
  <!-- JS -->
  <script src="{{ asset('jquery.min.js') }}"></script>
  <script type="text/javascript" src="//maps.google.com/maps/api/js?sensor=true"></script>
  <script src="{{ asset('gmaps.js') }}"></script>
  <script src="{{ asset('materialize.js') }}"></script>
  <script src="{{ asset('init.js') }}"></script>
  {% block js %}{% endblock %}
</body>
</html>
